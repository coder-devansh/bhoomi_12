{
  "name": "bhoomisetu",
  "version": "1.0.0",
  "description": "BhoomiSetu - Land Dispute Resolution Platform",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd new_backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "dev": "npm run dev:backend",
    "dev:backend": "cd new_backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm install && npm run build",
    "start": "cd new_backend && npm start",
    "start:prod": "npm run build && npm start",
    "heroku-postbuild": "npm run install:all && npm run build",
    "vercel-build": "cd frontend && npm install && npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["land-dispute", "legal-tech", "ai", "blockchain"],
  "author": "BhoomiSetu Team",
  "license": "MIT"
}
